{%  if isSeller.store_status is defined %}
 {% if isSeller.store_status %}
<div class="panel panel-default profile--panel">
  <h1 class="panel-heading">&nbsp;{{ text_selleroption }}</h1>	
  <a class="list-group-item" href="{{ dashboardicons }}"><i class="fa fa-bars" aria-hidden="true" style="padding-right: 10px"></i>{{ text_dashboard_icon }}</a>
  <a class="list-group-item" href="{{ dashboard }}"><i class="fa fa-bar-chart" aria-hidden="true" style="padding-right: 10px"></i>{{ text_dashboard }}</a>
  <a class="list-group-item" href="{{ sellerprofile }}"><i class="fa fa-address-card" aria-hidden="true" style="padding-right: 10px"></i>{{ text_sellerprofile }}</a>
  <a class="list-group-item" href="{{ sellerstore }}" ><i class="fa fa-list-alt" aria-hidden="true" style="padding-right: 10px"></i>{{ text_sellerstore }}</a> 
  <a class="list-group-item" href="{{ sellerproduct }}" ><i class="fa fa-archive" aria-hidden="true" style="padding-right: 10px"></i>{{ text_sellerproduct }}</a>

  <a class="list-group-item" href="{{ sellerorder }}" ><i class="fa fa-handshake-o" aria-hidden="true" style="padding-right: 10px"></i>{{ text_sellerorder }}</a>

  <a class="list-group-item" href="{{ sellerview }}" target="_blank" ><i class="fa fa-eye" aria-hidden="true" style="padding-right: 10px"></i>{{ text_sellerview }}</a>
  <a class="list-group-item" href="{{ sellerreview }}" ><i class="fa fa-comment" aria-hidden="true" style="padding-right: 10px"></i>{{ text_sellerreview }}</a>
  <a class="list-group-item" href="{{ sellerenquiry }}" ><i class="fa fa-envelope-open" aria-hidden="true" style="padding-right: 10px"></i>{{ text_sellerenquiry }}</a>
  <a class="list-group-item" href="{{ removeseller }}" onclick="return confirm('Are you sure?')" ><b><i class="fa fa-times" aria-hidden="true" style="padding-right: 10px"></i>{{ text_removeseller }}</b></a>
  </div>
  {% else %}
 <div class="pts-list-group">
  {% if isSeller.is_removed %}
  <a class="list-group-item" href="{{ becomeseller }}"><i class="fa fa-user-plus" aria-hidden="true" style="padding-right: 10px"></i>{{ text_becomeseller }}</a>
 {% else %}
  <a class="list-group-item" javascript="void(0)"><i class="fa fa-check" aria-hidden="true" style="padding-right: 10px"></i>{{ text_approval }}</a>
  {% endif %}
  </div>
  {% endif %}
  
 <script>
 $(document).ready(function(){
	
		if ($(window).width() < 768) {
		   $('.pts-list-group').parent().addClass('pts-column-right-sellerpanel');
		}
	});
 </script>
 <style>
 @media only screen and (max-width: 760px) {
	 .pts-column-right-sellerpanel,#column-right {
		display:block !important;
		margin: 0 auto;
		width: 100%;
		padding: 30px;
		z-index: 99 !important;
	 }
 }
 </style>
  {% endif %} 
  
  {% if logged %}
 {% if module_purpletree_multivendor_status %}
 {% if isSeller.store_status is empty %}
<div class="panel panel-default profile--panel">
 <a class="list-group-item" href="{{ becomeseller }}" >{{ text_becomeseller }}</a>
  </div>
 <script>
 $(document).ready(function(){
	
		if ($(window).width() < 768) {
		   $('.pts-list-group').parent().addClass('pts-column-right-sellerpanel');
		}
	});
 </script>
 <style>
 @media only screen and (max-width: 760px) {
	 .pts-column-right-sellerpanel,#column-right {
		display:block !important;
		margin: 0 auto;
		width: 100%;
		padding: 30px;
		z-index: 99 !important;
	 }
 }
 </style>
 {% endif %}
 {% endif %}
 {% endif %}
 